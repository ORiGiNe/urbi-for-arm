	for d in $$l1 $$l2;				\
	do						\
	  base=$$(basename $$d);			\
	  dir=$$(dirname $$d);				\
	  tar -C $$dir					\
	    --exclude '*.md5' --exclude '*.map'		\
	    -cf - $$base				\
